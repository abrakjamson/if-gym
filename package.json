{
  "name": "if-gym",
  "version": "0.1.0",
  "description": "AI agent framework for playing interactive fiction games",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveLeve/if-gym.git"
  },
  "keywords": [
    "interactive-fiction",
    "ai-agents",
    "llm",
    "text-adventures",
    "reinforcement-learning",
    "z-machine",
    "agent-framework"
  ],
  "author": "Steve Leve",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "start": "pnpm --filter @if-gym/cli start",
    "play": "pnpm --filter @if-gym/cli run play",
    "benchmark": "pnpm --filter @if-gym/cli run benchmark"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "tsup": "^8.0.0"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
